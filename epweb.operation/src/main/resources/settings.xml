<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
	<!-- 数据库设置 -->
	<entry key="db.url">jdbc:mysql://192.168.1.108:3306/smartwatch?characterEncoding=UTF-8</entry>
	<entry key="db.username">root</entry>
